<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
    <%@ taglib prefix="s" uri="/struts-tags"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html lang="en">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>Insert Employee</title>
</head>
<body>
<h1>Add a new Employee</h1>
<s:form method="post" action="insert">
<table>
<tr>
<td><s:textfield name="fname" label="First Name"></s:textfield></td>
</tr>
<tr>
<td><s:textfield name="lname" label="Last Name"></s:textfield></td>
</tr>
<tr>
<td><s:textfield name="deptid" label="Department Id"></s:textfield></td>
</tr>
<tr>
<td><s:textfield name="lanid" label="Lan Id"></s:textfield></td></tr>
<tr>
<td>
<s:textfield name="createdby" label="Created By"></s:textfield></td></tr>
<tr>
<td>
<s:textfield name="modifiedby" label="Modified By"></s:textfield>
</td>
</tr>
<tr>
<td>
<s:textfield name="email" label="Email Id"></s:textfield></td></tr>
 <tr>  
           
          <td><input type="submit" value="Save" /></td>  
         </tr>
</table>
</s:form>
<p><a href="index.jsp">Home</a></p>
</body>
</html>